{
  "name": "ant-design-pro",
  "version": "5.0.0-beta.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.14.5",
    "@ant-design/pro-descriptions": "^1.8.7",
    "@ant-design/pro-form": "^1.31.2",
    "@ant-design/pro-layout": "^6.21.1",
    "@ant-design/pro-list": "^1.10.6",
    "@ant-design/pro-skeleton": "^1.0.1",
    "@ant-design/pro-table": "^2.43.4",
    "@umijs/route-utils": "^1.0.37",
    "antd": "^4.16.7",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "qs": "^6.10.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.9",
    "umi": "^3.5.8",
    "umi-request": "^1.3.9",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.4",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/qs": "^6.9.7",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "^6.1.2",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.30.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.3.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.0.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
